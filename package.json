{
  "name": "roulette",
  "version": "1.0.0",
  "description": "Roulette REST API",
  "author": "Christian Barrios <christiandbf@hotmail.com>",
  "license": "MIT",
  "bugs": "https://github.com/christiandbf/roulette/issues",
  "homepage": "https://github.com/christiandbf/roulette/blob/master/README.md",
  "repository": "github.com:christiandbf/roulette.git",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "tsc": "tsc",
    "lint": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build": "npm-run-all clean lint prettier-format tsc",
    "dev": "nodemon --watch src -e ts --exec ts-node ./src/api/server.ts",
    "start": "node ./dist/api/server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "prettier --config .prettierrc --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/compression": "^1.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express-validator": "^3.0.0",
    "@types/express": "^4.17.1",
    "@types/ioredis": "^4.14.9",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.9.5",
    "@types/uuid": "^7.0.2",
    "aws-sdk": "^2.691.0",
    "compression": "^1.7.4",
    "dotenv": "^8.1.0",
    "express-validator": "^6.5.0",
    "express": "^4.10.1",
    "ioredis": "^4.16.1",
    "morgan": "^1.9.1",
    "shallow-equal-object": "^1.1.1",
    "typescript": "^3.5.3",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0"
  }
}
